{
  "name": "bedrock-kms-test",
  "version": "0.0.1-0",
  "description": "Bedrock KMS test",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=lcov npm test",
    "coverage-report": "nyc report"
  },
  "dependencies": {
    "bedrock": "^4.1.1",
    "bedrock-did-context": "^2.0.0",
    "bedrock-jsonld-document-loader": "^1.0.1",
    "bedrock-kms": "file:..",
    "bedrock-mongodb": "^8.0.0",
    "bedrock-ledger-context": "^18.0.0",
    "bedrock-package-manager": "^1.0.1",
    "bedrock-security-context": "^4.0.0",
    "bedrock-ssm-mongodb": "digitalbazaar/bedrock-ssm-mongodb#add-ed25519-verification-key-2020",
    "bedrock-veres-one-context": "^10.0.1",
    "bedrock-test": "^5.2.0",
    "cross-env": "^7.0.2",
    "nyc": "^15.0.1"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock-kms/**"
    ],
    "exclude": [
      "node_modules/bedrock-kms/node_modules/**"
    ]
  }
}
