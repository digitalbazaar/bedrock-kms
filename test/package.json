{
  "name": "bedrock-kms-test",
  "version": "0.0.1-0",
  "type": "module",
  "description": "Bedrock KMS test",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test c8 npm test",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly --reporter=text-summary --reporter=text npm test",
    "coverage-report": "c8 report"
  },
  "dependencies": {
    "@bedrock/core": "^6.0.1",
    "@bedrock/did-context": "^5.0.0",
    "@bedrock/did-io": "^10.1.0",
    "@bedrock/https-agent": "^4.0.0",
    "@bedrock/jsonld-document-loader": "^4.0.0",
    "@bedrock/kms": "file:..",
    "@bedrock/ledger-context": "^24.0.0",
    "@bedrock/mongodb": "^10.0.0",
    "@bedrock/package-manager": "^3.0.0",
    "@bedrock/security-context": "^8.0.0",
    "@bedrock/ssm-mongodb": "^10.0.0",
    "@bedrock/test": "^8.0.5",
    "@bedrock/veres-one-context": "^15.0.0",
    "@digitalbazaar/webkms-switch": "digitalbazaar/webkms-switch#make-context-optional",
    "bnid": "^3.0.0",
    "c8": "^9.1.0",
    "cross-env": "^7.0.3",
    "crypto-ld": "^7.0.0",
    "klona": "^2.0.5",
    "uuid": "^9.0.0"
  },
  "c8": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/@bedrock/kms/**"
    ],
    "exclude": [
      "node_modules/@bedrock/kms/node_modules/**"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ]
  }
}
