  "dependencies": {
    "@digitalbazaar/ed25519-verification-key-2018": "^3.1.1",
    "@digitalbazaar/ed25519-verification-key-2020": "^3.1.0",
    "@digitalbazaar/webkms-context": "^2.0.0",
    "@digitalbazaar/webkms-switch": "^9.0.3",
    "@digitalbazaar/x25519-key-agreement-key-2019": "^5.1.1",
    "@digitalbazaar/x25519-key-agreement-key-2020": "^2.0.0",
    "aes-key-wrapping-2019-context": "^1.0.3",
    "bedrock": "^4.3.0",
    "bedrock-did-context": "^2.0.0",
    "bedrock-did-io": "^6.0.1",
    "bedrock-jsonld-document-loader": "^1.0.1",
    "bedrock-kms": "file:..",
    "bedrock-ledger-context": "^21.0.1",
    "bedrock-mongodb": "^8.4.0",
    "bedrock-package-manager": "^1.0.1",
    "bedrock-security-context": "^5.0.0",
    "bedrock-ssm-mongodb": "^7.0.1",
    "bedrock-test": "^6.0.0",
    "bedrock-veres-one-context": "^12.0.0",
    "bnid": "^2.0.0",
    "cross-env": "^7.0.3",
