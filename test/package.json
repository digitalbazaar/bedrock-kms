{
  "name": "bedrock-kms-test",
  "version": "0.0.1-0",
  "description": "Bedrock KMS test",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm test",
